{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/api/resume/route.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/resume/**/*.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/ai-cover-letter/[id]/**/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}